{
    "root": true,
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jasmine": true
    },
    "plugins": [
        "babel",
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}
